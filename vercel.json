{
    "builds": [{
        "src": "com_br_easycontroller_app/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb",  "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "com_br_easycontroller_app/wsgi.py"
        }
    ]
}